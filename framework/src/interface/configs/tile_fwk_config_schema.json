        "runtime": {
            "type": "object",
            "label": {
                "zh": "设置运行时优化参数信息",
                "en": "Set Runtime Optimization Parameters"
            },
            "properties": {
                "device_sched_mode": {
                    "type": "integer",
                    "label": {
                        "zh": "设置计算子图的调度模式",
                        "en": "Set the schedule mode for subgraphs"
                    },
                    "minimum": 0,
                    "maximum": 3,
                    "options": [
                        {
                            "value": 0,
                            "label": {
                                "zh": "默认调度模式:ready子图放入共享队列，各个调度线程抢占子图进行发送，子图获取发送遵循先入先出",
                                "en": "default schedule mode:Ready subgraph enter a shared queue. Schedule threads fetch and dispatch subgraph on first-in-first-out basis"
                            }
                        },
                        {
                            "value": 1,
                            "label": {
                                "zh": "L2cache亲和调度模式:选择最新依赖ready的子图优先下发，达到复用L2cache的效果",
                                "en": "L2 cache affinity schedule mode: prioritizes dispatching the most recently dependency-ready subgraph to improve L2 cache reuse"
                            }
                        },
                        {
                            "value": 2,
                            "label": {
                                "zh": "公平调度模式：下发子图时尽量控制多线程间的公平性，但会带来额外的调度开销",
                                "en": "Fair schedule mode: maintains fairness across threads when dispatching subgraph, at the cost of additional schedule overhead"
                            }
                        },
                        {
                            "value": 3,
                            "label": {
                                "zh": "公平调度兼L2cache亲和调度模式:同时开启L2cache亲和调度模式以及公平调度模式",
                                "en": "Fair & L2 cache affinity schedule mode: enables both L2 cache affinity and fair schedule"
                            }
                        }
                    ]
                },
                "stitch_function_inner_memory": {
                    "type": "integer",
                    "label": {
                        "zh": "控制root function中间计算结果的内存池大小的参数，该数值越大root function运行时并行度越高",
                        "en": "Control the memory pool size for intermediate results of rootfunction; a larger value enables higher runtime parallelism of rootfunction"
                    },
                    "minimum": 1,
                    "maximum": 2147483647
                },
                "stitch_function_outcast_memory": {
                    "type": "integer",
                    "label": {
                        "zh": "控制stitch构建的devicetask中间计算结果（devitask内部rootfunction的outcast）的内存池大小的参数，该数值越大devicetask下发的rootfunction越多",
                        "en": "Control the memory pool size for intermediate results inside stitch-constructed device task; a larger value allows more rootfunction to be dispatched per device task"
                    },
                    "minimum": 1,
                    "maximum": 2147483647
                },
                "stitch_function_num_initial": {
                    "type": "integer",
                    "label": {
                        "zh": "设置运行时ctrlflow aicpu里stitch构建首个device task的rootfunction数量，进而控制device machine启动头开销大小",
                        "en": "Set the number of rootfunction generated for the first device task in ctrlflow AICPU, thereby controlling the startup overhead of device machine"
                    },
                    "minimum": 1,
                    "maximum": 128
                },
                "stitch_function_num_step": {
